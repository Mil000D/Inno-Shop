{
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/auth/{everything}",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamScheme": "https",
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7001
        }
      ],
      "SwaggerKey": "auth"
    },
    {
      "UpstreamPathTemplate": "/gateway/users/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "DownstreamScheme": "https",
      "DownstreamPathTemplate": "/api/users/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7001
        }
      ],
      "FileCacheOptions": {
        "TtlSeconds": 10
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      },
      "RouteClaimsRequirement": {
        "Role": "Admin"
      },
      "SwaggerKey": "users"
    },
    {
      "UpstreamPathTemplate": "/gateway/products/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "DownstreamPathTemplate": "/api/products/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7002
        }
      ],
      "FileCacheOptions": {
        "TtlSeconds": 10
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      },
      "RouteClaimsRequirement": {
        "Role": "User"
      },
      "SwaggerKey": "products"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7000"
  },
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Auth API",
          "Version": "v1",
          "Url": "https://localhost:7001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "users",
      "Config": [
        {
          "Name": "Users API",
          "Version": "v1",
          "Url": "https://localhost:7001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "products",
      "Config": [
        {
          "Name": "Products API",
          "Version": "v1",
          "Url": "https://localhost:7002/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}